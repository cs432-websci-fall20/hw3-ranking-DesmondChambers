<script>
  var pathname;

  if (window.asset && window.asset.url) {
    // hybrid
    pathname = window.asset.url;
  } else {
    // vi
    pathname = window.location.pathname;
  }

  var tags = document.getElementsByClassName('at-home-nav__li_link');

  for (var index = 0; index < tags.length; index++) {
    var el = tags[index];

    if (pathname.length && el && el.href && el.href.includes(pathname)) {
      el.classList.add('at-home-nav__li--current');
      break;
    }
  }

  // Add and remove gradient on end of div
  var nav = document.getElementById('at-home-nav');
  var gradientDivRight =  document.querySelector('.at-home-nav__mobile-cutoff-gradient--right');
  var gradientDivLeft =  document.querySelector('.at-home-nav__mobile-cutoff-gradient--left');
  var showGradientThreshold = 75;

  if (nav && gradientDivLeft && gradientDivRight) {
    setGradientOpacities();

    nav.addEventListener('scroll', function (e) {
      var scrollingWidth = nav.scrollWidth - nav.clientWidth;
  
      if (scrollingWidth < showGradientThreshold) return;

      // Change x coords, since they need to follow the scrolling
      gradientDivRight.style.right = -Math.abs(nav.scrollLeft) + 'px';
      gradientDivLeft.style.left = Math.abs(nav.scrollLeft) + 'px';

      setGradientOpacities();
    });
  }

  function setGradientOpacities () {
    var scrollingWidth = nav.scrollWidth - nav.clientWidth;
  
    if (scrollingWidth < showGradientThreshold) return;

    // Calculating when to start gradually fading the div so the transition is smooth
    var scrollLeft = nav.scrollLeft;
    var fadeStart = 0.8 * scrollingWidth;
    var fadeLength = scrollingWidth - fadeStart;

    setRightGradientOpacity(scrollLeft, fadeStart, fadeLength);
    setLeftGradientOpacity(scrollLeft, fadeStart, fadeLength);
  }

  function setLeftGradientOpacity (currentX, startFadeX, fadeLength) {
    var fadePct = currentX / fadeLength;
    var opacity = 1 * fadePct;

    if (currentX >= fadeLength) {
      // Just in case something weird happens where it does't get set to 1 otherwise
      gradientDivLeft.style.opacity = 1;
    } else {
      gradientDivLeft.style.opacity = opacity < 0 ? 0 : opacity; 
    }
  }

  function setRightGradientOpacity (currentX, startFadeX, fadeLength) {
    var fadePct = (currentX - startFadeX) / fadeLength;
    var opacity = 1 - fadePct;

    if (currentX >= startFadeX) {
      gradientDivRight.style.opacity = opacity < 0 ? 0 : 1 - fadePct; 
    } else {
      // Just in case something weird happens where it does't get set to 1 otherwise
      gradientDivRight.style.opacity = 1;
    }
  }
</script>
