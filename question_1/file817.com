id Of?\"\u003C\u002Fp\u003E\r\n\u003Cp\u003EHer hobbies include singing, and she is married with three children.\u003C\u002Fp\u003E","contactDetails":{"type":"id","generated":true,"id":"$Person:UGVyc29uOm55dDovL3BlcnNvbi81M2U0ZmU5OC0wNzBjLTU3ZTYtOWEzMy1lZmY4MmViMTkwZjk=.contactDetails","typename":"ContactDetails"},"legacyData":{"type":"id","generated":true,"id":"$Person:UGVyc29uOm55dDovL3BlcnNvbi81M2U0ZmU5OC0wNzBjLTU3ZTYtOWEzMy1lZmY4MmViMTkwZjk=.legacyData","typename":"PersonLegacyData"}},"Image:SW1hZ2U6bnl0Oi8vaW1hZ2UvOTE2MjI2OTYtYWRhMC01MjRmLTk0Y2ItOWEwMGRjNzI5YTNj":{"id":"SW1hZ2U6bnl0Oi8vaW1hZ2UvOTE2MjI2OTYtYWRhMC01MjRmLTk0Y2ItOWEwMGRjNzI5YTNj","crops({\"renditionNames\":\"thumbLarge\"})":[{"type":"id","generated":true,"id":"Image:SW1hZ2U6bnl0Oi8vaW1hZ2UvOTE2MjI2OTYtYWRhMC01MjRmLTk0Y2ItOWEwMGRjNzI5YTNj.crops({\"renditionNames\":\"thumbLarge\"}).0","typename":"ImageCrop"}],"__typename":"Image","crops({\"renditionNames\":[\"thumbStandard\",\"thumbLarge\",\"mobileMasterAt3x\"]})":[{"type":"id","generated":true,"id":"Image:SW1hZ2U6bnl0Oi8vaW1hZ2UvOTE2MjI2OTYtYWRhMC01MjRmLTk0Y2ItOWEwMGRjNzI5YTNj.crops({\"renditionNames\":[\"thumbStandard\",\"thumbLarge\",\"mobileMasterAt3x\"]}).0","typename":"ImageCrop"}]},"ImageRendition:images20180712multimediaauthor-maggie-habermanauthor-maggie-haberman-thumbLarge.png":{"url":"https:\u002F\u002Fstatic01.nyt.com\u002Fimages\u002F2018\u002F07\u002F12\u002Fmultimedia\u002Fauthor-maggie-haberman\u002Fauthor-maggie-haberman-thumbLarge.png","name":"thumbLarge","__typename":"ImageRendition","width":150,"height":150},"Image:SW1hZ2U6bnl0Oi8vaW1hZ2UvOTE2MjI2OTYtYWRhMC01MjRmLTk0Y2ItOWEwMGRjNzI5YTNj.crops({\"renditionNames\":\"thumbLarge\"}).0":{"renditions":[{"type":"id","generated":false,"id":"ImageRendition:images20180712multimediaauthor-maggie-habermanauthor-maggie-haberman-thumbLarge.png","typename":"ImageRendition"}],"__typename":"ImageCrop"},"Person:UGVyc29uOm55dDovL3BlcnNvbi80ODU1ODc3OC1hYzUxLTVmZWYtODU4My1iODBlYWQ4ZGZkZmI=":{"id":"UGV