y9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.10":{"__typename":"ParagraphBlock","textAlign":"LEFT","content":[{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.10.content.0","typename":"TextInline"}]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.10.content.0":{"__typename":"TextInline","text":"By that same logic, the race today is teetering between a straightforward loss for the president and the worst defeat of an incumbent president since Herbert Hoover.","formats":[]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.11":{"__typename":"ParagraphBlock","textAlign":"LEFT","content":[{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.11.content.0","typename":"TextInline"},{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.11.content.1","typename":"TextInline"},{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.11.content.2","typename":"TextInline"},{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.11.content.3","typename":"TextInline"}]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.11.content.0":{"__typename":"TextInline","text":"More about Marquette. ","formats":[{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.11.content.0.formats.0","typename":"BoldFormat"}]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.11.content.0.formats.0":{"__typename":"BoldFormat","type":null},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.11.content.1":{"__typename":"TextInline","text":"It’s worth taking a second to contemplate this ","formats":[]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.11.content.2":{"__typename":"TextInline","text":"Wisconsin number","formats":[{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.11.content.2.formats.0","typename":"LinkFormat"}]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.11.content.2.formats.0":{"__typename":"LinkFormat","url":"https:\u002F\u002Flaw.marquette.edu\u002Fpoll\u002F","title":""},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.11.content.3":{"__typename":"TextInline","text":" from the Marquette Law poll. It’s the first result from a telephone survey in Wisconsin in several weeks, it’s from one of the best pollsters in the country, and the result is somewhat closer than we would have guessed.","formats":[]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.12":{"__typename":"ParagraphBlock","textAlign":"LEFT","content":[{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.12.content.0","typename":"TextInline"}]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.12.content.0":{"__typename":"TextInline","text":"In most of our discussions about the various roads to 270 electoral votes, we have mostly supposed that Wisconsin is part of Mr. Biden’s path of least resistance after several high-quality pollsters showed him solidly ahead in early September, just after the Republican convention. Since then, the national polls have shifted two to three points in Mr. Biden’s favor, so we would have guessed he was even further ahead today — maybe even up by eight to nine points.","formats":[]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.13":{"__typename":"ParagraphBlock","textAlign":"LEFT","content":[{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.13.content.0","typename":"TextInline"}]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.13.content.0":{"__typename":"TextInline","text":"Instead, the Marquette poll found an essentially unchanged race, with Mr. Biden up five points among registered and likely voters. That’s two points better for Mr. Trump among registered voters, compared with early September, and one point better for Mr. Biden among likely voters.","formats":[]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.14":{"__typename":"ParagraphBlock","textAlign":"LEFT","content":[{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.14.content.0","typename":"TextInline"}]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.14.content.0":{"__typename":"TextInline","text":"Now, it’s been a while since we’ve had many telephone surveys of Wisconsin, so we don’t have a particularly robust average in the state. Most of our assumptions about the race are based on polls that are fairly out of date, mainly conducted during or in the immediate aftermath of the unrest in Kenosha. So it’s possible that the president’s standing is relatively resilient in Wisconsin.","formats":[]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.15":{"__typename":"ParagraphBlock","textAlign":"LEFT","content":[{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.15.content.0","typename":"TextInline"}]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.15.content.0":{"__typename":"TextInline","text":"If the president is indeed holding up well in Wisconsin, that would help keep one of his best remaining hopes alive: the chance that he can scratch out a victory in the Electoral College while losing the national popular vote by a much wider margin than he did four years ago. There are limits to what the president can realistically hope to pull off here — winning while losing the national vote by more than five points would be a real stretch. But if Mr. Trump really were down only by five points in Wisconsin, even as he trails by 10 nationwide, it would be one important step toward keeping that possibility alive.","formats":[]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.16":{"__typename":"ParagraphBlock","textAlign":"LEFT","content":[{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.16.content.0","typename":"TextInline"}]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.16.content.0":{"__typename":"TextInline","text":"We’ll have a Times\u002FSiena poll in Wisconsin early next week, so that ought to help clarify things.","formats":[]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.17":{"__typename":"ParagraphBlock","textAlign":"LEFT","content":[{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.17.content.0","typename":"TextInline"},{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.17.content.1","typename":"TextInline"}]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.17.content.0":{"__typename":"TextInline","text":"A daunting deficit. ","formats":[{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.17.content.0.formats.0","typename":"BoldFormat"}]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.17.content.0.formats.0":{"__typename":"BoldFormat","type":null},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.17.content.1":{"__typename":"TextInline","text":"You could go nuts trying to decide whether to believe the really, really bad polls for Mr. Trump or the merely bad polls for him, and that’s where the poll average comes in. By our estimates, he faces an increasingly daunting deficit with under a month until the election. He trails by nearly 10 points nationwide.","formats":[]},"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL2JiYzg5OTY2LTc2YWQtNThkZi04NTM3LTI5ZWM2MzhhNDYxMw==.body.content@filterEmpty.18":{"__typename":"ParagraphBlock","textAlign":"LEFT","content":[{"type":"id","generated":true,"id":"$Article:QXJ0aWNsZTpueXQ6Ly9hcnRpY2xlL